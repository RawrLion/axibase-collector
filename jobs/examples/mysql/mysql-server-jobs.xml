<list>
    <com.axibase.collector.model.job.jdbc.JdbcJobConfig>
        <name>mysql-performance-schema</name>
        <cronExpression>0 0/15 * * * ?</cronExpression>
        <enabled>true</enabled>
        <jobType>JDBC</jobType>
        <defaultTags/>
        <checkLastTime>false</checkLastTime>
        <commitInterval>10</commitInterval>
        <errorThreshold>0</errorThreshold>
        <databaseConfiguration>
            <name>mysql-www-axibase</name>
            <type>MYSQL</type>
            <server>www.axibase.com</server>
            <port>3306</port>
            <database>performance_schema</database>
            <dbUser>axibase</dbUser>
            <dbPasswd>password</dbPasswd>
            <testQuery>select 1</testQuery>
            <maxActive>4</maxActive>
            <maxIdle>4</maxIdle>
            <minIdle>0</minIdle>
            <initialSize>0</initialSize>
            <maxWaitSeconds>60</maxWaitSeconds>
            <maxAgeMinutes>60</maxAgeMinutes>
            <loginTimeoutSeconds>60</loginTimeoutSeconds>
            <idleTimeoutSeconds>60</idleTimeoutSeconds>
            <socketTimeoutSeconds>0</socketTimeoutSeconds>
            <active>0</active>
            <idle>0</idle>
            <driverClassName>oracle.jdbc.OracleDriver</driverClassName>
        </databaseConfiguration>
        <configurations>
            <com.axibase.collector.model.job.jdbc.QueryConfiguration>
                <query>SELECT * FROM global_status WHERE variable_name NOT LIKE &apos;Innodb_%_status&apos; &#xd;
                    AND variable_name NOT IN (&apos;Max_used_connections_time&apos;, &apos;Ssl_server_not_after&apos;,
                    &apos;Ssl_server_not_before&apos;, &apos;Ssl_session_cache_mode&apos;)
                </query>
                <defaultEntity>axibase.com</defaultEntity>
                <tagColumns/>
                <defaultTags/>
                <metricColumns/>
                <ignoredMetricColumns/>
                <metricNameColumn>VARIABLE_NAME</metricNameColumn>
                <metricValueColumn>VARIABLE_VALUE</metricValueColumn>
                <timeType>NONE</timeType>
                <metricPrefix>mysql.global_status.</metricPrefix>
                <queryWithTime>false</queryWithTime>
                <checkLastTime>false</checkLastTime>
                <lastTimeIntervalSeconds>0</lastTimeIntervalSeconds>
                <queryTimeoutSeconds>-1</queryTimeoutSeconds>
                <maxRows>-1</maxRows>
                <putType>SERIES</putType>
                <propertyKeyColumns/>
                <propertyValueColumns/>
                <ignoredPropertyValueColumns/>
            </com.axibase.collector.model.job.jdbc.QueryConfiguration>
        </configurations>
    </com.axibase.collector.model.job.jdbc.JdbcJobConfig>
</list>