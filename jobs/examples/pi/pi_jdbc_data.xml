    <configurations>
      <com.axibase.collector.model.job.jdbc.QueryConfiguration>
        <query>SELECT TOP 100000 __utctime, tag, value, &#xd;
CASE WHEN questionable = true THEN true ELSE NULL END AS questionable,&#xd;
CASE WHEN annotated = true THEN true ELSE NULL END AS annotated,&#xd;
CASE WHEN substituted = true THEN true ELSE NULL END AS substituted,&#xd;
CASE WHEN annotated = true THEN annotations ELSE NULL END AS annotations,&#xd;
CASE WHEN _index &gt; 1 THEN _index ELSE NULL END AS _index,&#xd;
CASE WHEN status &lt;&gt; 0 THEN status ELSE NULL END AS status&#xd;
FROM piarchive..picomp2&#xd;
WHERE __utctime &gt; ?&#xd;
AND tag &lt;&gt; &apos;FUTURE&apos;&#xd;
ORDER BY time</query>
